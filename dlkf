[32m+[m[32m// Initialize Firebase[m
[32m+[m[32mfirebase.initializeApp(firebaseConfig);[m
[32m+[m
[32m+[m[32mlet database = firebase.database();[m
[32m+[m
[32m+[m[32m/* If a user is signed in[m
[32m+[m[32m    - show the favorites drop down[m
[32m+[m[32m    - If that user has favorites in the database[m
[32m+[m[32m        - Call the function to populate the dropdown but buttons. */[m
[32m+[m
[32m+[m[32m// Pushes parameters to database.[m
[32m+[m[32mfunction pushToDatabase(username, resort) {[m
[32m+[m[32m    let newFavRef = database.ref(username + '/favorites/' + resort);[m
[32m+[m[32m    newFavRef.set({[m
[32m+[m[32m        resort: resort[m
[32m+[m[32m    });[m
[32m+[m[32m    $('input').val('');[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Remove a child from the database.[m
[32m+[m[32mfunction removeFromDb(username, resort) {[m
[32m+[m[32m    database.ref(username + '/favorites/' + resort).remove();[m
[32m+[m[32m    $('input').val('');[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m$('#add').on('click', (event) => {[m
[32m+[m[32m    event.preventDefault();[m
[32m+[m[32m    pushToDatabase('kramer', $('input').val());[m
[32m+[m[32m    console.log('You clicked');[m
[32m+[m[32m})[m
[32m+[m[32m$('#remove').on('click', (event) => {[m
[32m+[m[32m    event.preventDefault();[m
[32m+[m[32m    removeFromDb('kramer', $('input').val());[m
[32m+[m[32m    console.log('You removed');[m
[32m+[m[32m})[m
